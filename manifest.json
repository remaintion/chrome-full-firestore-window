{
  "manifest_version": 2,
  "name": "Firestore Full Screen Plugin",
  "description": "This extension will extend your firestore console to full screen!",
  "version": "1.0.3",
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "optional_permissions": [
    "tabs"
  ]
}